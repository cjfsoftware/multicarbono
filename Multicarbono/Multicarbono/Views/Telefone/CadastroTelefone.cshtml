@model Multicarbono.Models.Telefone.Telefone

@Html.Partial("_Menu")

<hr />
<h6 style="margin-left:10%">Cadastrar Telefone</h6><br /><br />
<div class="row" style="margin-left:10%">
    <div class="col-md-4">
        <form asp-action="CadastroTelefone">
            <div class="form-group">
                <label asp-for="IdCliente" class="control-label"></label>
                <input asp-for="IdCliente" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="NumTelefone" class="control-label"></label>
                <input asp-for="NumTelefone" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Contato" class="control-label"></label>
                <input asp-for="Contato" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Criar telefone" class="btn btn-info" />
            </div>
        </form>
    </div>
</div>

