@*@model IEnumerable<Multicarbono.Models.Telefone.Telefone>*@

@{
    Layout = null;
    List<Multicarbono.Models.Telefone.Telefone> itemTelefone = Newtonsoft.Json.JsonConvert.DeserializeObject<List<Multicarbono.Models.Telefone.Telefone>>(TempData["Telefone"].ToString());
}

<br />
<h6 style="margin-left:2%">Contatos</h6>


<table class="table table-striped table-hover" style="margin-left:1%; margin-right:1%">
    <thead>
        <tr>
            <th>
                @*@Html.DisplayNameFor(itemTelefone => itemTelefone.Contato)*@
                Contato
            </th>
            <th>
                @*@Html.DisplayNameFor(itemTelefone => itemTelefone.NumTelefone)*@
                NumTelefone
            </th>
            <th>
                @*@Html.DisplayNameFor(itemTelefone => itemTelefone.Email)*@
                Email
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in itemTelefone.Select((value, index) => new { value, index }))
        {
            <td>
                @Html.DisplayFor(modelItem => item.value.Contato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.value.NumTelefone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.value.Email)
            </td>
        }
    </tbody>
</table>
