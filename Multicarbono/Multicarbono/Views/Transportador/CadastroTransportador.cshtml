@model Multicarbono.Models.Transportador.Transportador

@Html.Partial("_Menu")

<hr />
<h6 style="margin-left:10%">Cadastrar Transportador</h6><br /><br />
<div class="row" style="margin-left:10%">
    <div class="col-md-4">
        <form asp-action="CadastroTransportador">
            <div class="form-group">
                <label asp-for="CNPJTransportador" class="control-label"></label>
                <input asp-for="CNPJTransportador" class="form-control" required/>
            </div>
            <div class="form-group">
                <label asp-for="RazaoSocial" class="control-label"></label>
                <input asp-for="RazaoSocial" class="form-control" required/>
            </div>
            <div class="form-group">
                <label asp-for="IETransportador" class="control-label"></label>
                <input asp-for="IETransportador" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="EnderecoTransp" class="control-label"></label>
                <input asp-for="EnderecoTransp" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Criar transportador" class="btn btn-info" />
            </div>
        </form>
    </div>
</div>

